page {
  background: #F2F3F5;
}
page .container {
  width: 100%;
  background: #F2F3F5;
  padding-bottom: 550rpx;
}
page .container .tab-bar {
  width: 100%;
  border-bottom: 2rpx solid #000;
  background-color: #fff;
}
page .container .info-box {
  width: 100%;
  background-color: #fff;
}
page .container .info-box .info-item {
  width: 90%;
  margin-left: 40rpx;
  font-size: 32rpx;
  color: #000000;
  padding: 28rpx 40rpx 40rpx 0;
  border-top: 2rpx solid #EDEFF0;
  line-height: 42rpx;
}
page .container .info-box .info-item .icon {
  width: 30rpx;
  height: 30rpx;
}
page .container .info-box .info-item .right-num {
  display: inline-block;
  float: right;
}
page .container .info-box .info-item .right-num .weui-btn {
  vertical-align: 10%;
  padding-right: 20rpx;
}
page .container .info-box .info-item .right-num .search-input {
  display: inline-block;
  text-align: right;
  padding-right: 20rpx;
}
page .container .info-box .info-item .right-num .item-unit {
  float: right;
}
page .container .info-box .info-item .right-num image {
  width: 18rpx;
  height: 32rpx;
}
page .container .info-box .info-item .right-num .input-num {
  margin-right: 20rpx;
}
page .container .info-box .middle-bar {
  width: 100%;
  height: 20rpx;
  background: #F2F3F5;
}
page .container .tip-box {
  position: fixed;
  z-index: 9;
  width: 690rpx;
  border-radius: 8rpx;
  background: #fff;
  top: 234rpx;
  padding: 40rpx 0;
  /**index.wxss**/
  /* 表头 */
  /* 表格cell */
}
page .container .tip-box image {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  right: 40rpx;
  top: 40rpx;
}
page .container .tip-box .tip-top {
  font-size: 36rpx;
  color: #2C2F37;
  padding: 0 0 40rpx 40rpx;
  border-bottom: 2rpx solid #EDEFF0;
}
page .container .tip-box .tip-text {
  font-size: 32rpx;
  color: #475266;
  line-height: 45rpx;
  width: 90%;
  margin: 0 auto;
  padding-top: 40rpx;
}
page .container .tip-box .title {
  width: 100%;
  text-align: center;
  font-size: 50rpx;
  font-weight: 900;
}
page .container .tip-box .tableView {
  border: 1px solid #E3E5E6;
  border-right: 0;
  border-bottom: 0;
  width: 98%;
  margin-left: 1%;
}
page .container .tip-box .table_header {
  display: flex;
  justify-content: space-between;
  /* 项目位于各行之间留有空白的容器内。 */
  background: #F2F3F5;
  width: 100%;
  font-size: 28rpx;
  color: #2C2F37;
  text-align: center;
  font-weight: 900;
}
page .container .tip-box .th {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 80rpx;
  border-bottom: 1px solid #E3E5E6;
  border-right: 1px solid #E3E5E6;
}
page .container .tip-box .th1,
page .container .tip-box .th2 {
  width: 50%;
}
page .container .tip-box .th3 {
  width: 50%;
}
page .container .tip-box .table_cell {
  display: flex;
  justify-content: space-between;
  /* 项目位于各行之间留有空白的容器内。 */
  width: 100%;
  font-size: 28rpx;
  color: #878D99;
  text-align: center;
}
page .container .tip-box .td {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 60rpx;
  border-bottom: 1px solid #E3E5E6;
  border-right: 1px solid #E3E5E6;
}
page .container .tip-box .td1,
page .container .tip-box .td2 {
  width: 50%;
}
page .container .tip-box .td3 {
  width: 50%;
}
page .container .cal-button {
  width: 610rpx;
  margin: 0 auto;
  padding: 27rpx 0;
  text-align: center;
  margin-top: 42rpx;
  border-radius: 50rpx;
}
page .container .gary {
  color: #fff;
  background: #878D99;
}
page .container .blue {
  color: #fff;
  background: #6595F4;
}
page .container .swiper-beijing {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  position: fixed;
  top: 0rpx;
  z-index: 1;
}
